{
  "name": "ethereal-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^2.2.4",
    "@electron/remote": "^2.0.8",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.3",
    "electron-is-dev": "^2.0.0",
    "framer-motion": "^6.5.1",
    "node-jre": "^0.2.3",
    "progress": "^2.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^3.11.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-spinners": "^0.13.3",
    "rmdir": "^1.2.0",
    "tar-fs": "^2.1.1",
    "unzipper": "^0.10.11",
    "web-vitals": "^0.2.4",
    "zlib": "^1.0.5"
  },
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none npm run start\" \"npm run electron:start\"",
    "electron:build": "npm run build && electron-builder -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on tcp:3000 && electron ."
  },
  "build": {
    "appId": "ethereal-client",
    "productName": "Ethereal Client",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/favicon.ico"
    },
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "nsis": {
      "oneClick": true,
      "installerIcon": "build/favicon.ico",
      "uninstallerIcon": "build/favicon.ico",
      "uninstallDisplayName": "EtherealClientUninstaller",
      "license": "license.md",
      "allowToChangeInstallationDirectory": false
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "electron": "^19.0.8",
    "electron-builder": "^23.1.0",
    "wait-on": "^6.0.1"
  }
}
